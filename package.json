{
  "name": "markdown-it-task-lists",
  "version": "2.1.1",
  "description": "A markdown-it plugin to create GitHub-style task lists",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -r dist && mkdir dist",
    "build": "tsc && npm run build-dist",
    "build-dist": "npm run build-dist-file && npm run build-dist-min",
    "build-dist-file": "esbuild src/index.ts --bundle --format=iife --global-name=markdownitTaskLists --outfile=dist/index.js --banner:js=\"/*! ${npm_package_name} ${npm_package_version} ${npm_package_homepage} by ${npm_package_author} @license ${npm_package_license} */\"",
    "build-dist-min": "esbuild dist/index.js --minify --outfile=dist/index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:revin/markdown-it-task-lists.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "@types/markdown-it": "^14.1.2",
    "cheerio": "^1.1.2",
    "esbuild": "^0.27.2",
    "jest": "^30.2.0",
    "linkedom": "^0.18.12",
    "markdown-it": "^14.1.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "task",
    "list",
    "todo",
    "checkbox"
  ],
  "author": "Revin Guillen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/revin/markdown-it-task-lists/issues"
  },
  "homepage": "https://github.com/revin/markdown-it-task-lists#readme"
}
